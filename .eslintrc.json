{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "off",
        "max-len": "off",
        "no-restricted-syntax": "off",
        "import/no-dynamic-require": "off",
        "consistent-return": "off",
        "no-param-reassign": "off",
        "no-console": "off",
        "global-require": "off",
        "no-underscore-dangle": "off",
        "no-plusplus": "off",
        "no-unused-expressions": "off",
        "import/no-unresolved": "off", // needs due to guild.js file being ignored
        "import/extensions": "off" // needs due to guild.js file being ignored
    }
}
